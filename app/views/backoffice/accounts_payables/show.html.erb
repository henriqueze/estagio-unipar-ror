<h1 class="page-header">Contas a Pagar</h1>

<div class="panel panel-default">
  <div class="panel-heading">
    Visualizando Conta a Pagar
    <%= link_to backoffice_accounts_payables_path, class: "btn btn-warning btn-circle pull-right",
    style:"margin-top: -5px"  do %>
    <i class="fa fa-arrow-left"></i>
    <% end %>
  </div>
  <div class="panel-body">
    <p>
      <strong>State:</strong>
      <%= @accounts_payable.state %>
    </p>

    <p>
      <strong>Description:</strong>
      <%= @accounts_payable.description %>
    </p>

    <p>
      <strong>Document kind:</strong>
      <%= @accounts_payable.document_kind %>
    </p>

    <p>
      <strong>Document number:</strong>
      <%= @accounts_payable.document_number %>
    </p>

    <p>
      <strong>Issue date:</strong>
      <%= @accounts_payable.issue_date %>
    </p>

    <p>
      <strong>Expiration date:</strong>
      <%= @accounts_payable.expiration_date %>
    </p>

    <p>
      <strong>Payment date:</strong>
      <%= @accounts_payable.payment_date %>
    </p>

    <p>
      <strong>Total parcels:</strong>
      <%= @accounts_payable.total_parcels %>
    </p>

    <p>
      <strong>Current parcel:</strong>
      <%= @accounts_payable.current_parcel %>
    </p>

    <p>
      <strong>Payable value:</strong>
      <%= @accounts_payable.payable_value %>
    </p>

    <p>
      <strong>Additions value:</strong>
      <%= @accounts_payable.additions_value %>
    </p>

    <p>
      <strong>Discount value:</strong>
      <%= @accounts_payable.discount_value %>
    </p>

    <p>
      <strong>Total value:</strong>
      <%= @accounts_payable.total_value %>
    </p>

    <p>
      <strong>Paid value:</strong>
      <%= @accounts_payable.paid_value %>
    </p>

    <p>
      <strong>Voucher image:</strong>
      <%= @accounts_payable.voucher_image %>
    </p>

    <p>
      <strong>Purchase:</strong>
      <%= @accounts_payable.purchase %>
    </p>

  </div>
</div>
